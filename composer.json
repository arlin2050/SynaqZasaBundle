{
    "name": "synaq/zasa-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 wrapper for the Zimbra Soap Admin API (ZASA)",
    "keywords": ["API","Zimbra","Symfony2"],
    "homepage": "https://github.com/synaq/SynaqZasaBundle",
    "authors": [
        {
            "name": "Nic Parry",
            "email": "nicjparry@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "ext-curl": "*",
        "symfony/framework-bundle": ">=2.1",
        "mockery/mockery": "dev-master",
        "synaq/curl-bundle": "dev-master",
	"phpunit/phpunit": "3.7.*"
    },
    "require-dev": {
        "symfony/yaml": "2.1.*",
        "symfony/validator": "2.1.*"
    },
    "autoload": {
        "psr-0": { "Synaq\\ZasaBundle": "" }
    },
    "target-dir": "Synaq/ZasaBundle"
}
